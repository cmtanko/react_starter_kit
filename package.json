  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "react-start": "concurrently \"npm run server\" \"react-scripts start\"",
    "start": "nf start -p 3000",
    "server": "pouchdb-server -d ./db",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "winbuild": "electron-packager . react-starter-kit --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "flow": "flow",
    "precommit": "lint-staged",
    "heroku-postbuild": "npm install --only=dev && npm install && npm run build",
    "pack": "build --dir",
    "build": "react-scripts build",
    "dist": "npm run build && build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "winappbuild": "electron-windows-store --input-directory D:\\Personal\\React_Starter_kit\\react_starter_kit_Win\\react_starter_kit\\build --output-directory D:\\Personal\\React_Starter_kit\\react_starter_kit_Win\\react_starter_kit\\windows-store --flatten true"
  }
