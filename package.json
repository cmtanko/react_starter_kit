    "lint-staged": "^5.0.0",
    "mini.css": "^2.3.7",
    "prettier": "^1.8.2",
    "react": "^16.1.1",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
